package com.tsb.most.basebiz.dataitem.configuration;

import com.tsb.most.framework.dataitem.DataItem;

/**
* use TerminalDefinitionItem Class as parameters to CUD 
*
* @author Miss Nam-Sook Chang
* @version 1.0
*
* @see 
*/
public class TerminalDefinitionItem extends DataItem {
    private String ptyCd;
    private String tmnlTpCd;
    private String engPtyNm;
    private String compRegNo;
    private String rpstvNm;
    private String telNo;
    private String faxNo;
    private String email;
    private String webSite;
    private String addr;
    private String rmk;    
    private String crudFlag;
    private int loLen;
    private int loWth;
    private String unnoCd;
    private String userId;
    
    
    
    public String getUserId() {
        return userId;
    }
    public void setUserId(String userId) {
        this.userId = userId;
    }
  
    public int getLoLen() {
        return loLen;
    }
    public void setLoLen(int loLen) {
        this.loLen = loLen;
    }
    public int getLoWth() {
        return loWth;
    }
    public void setLoWth(int loWth) {
        this.loWth = loWth;
    }
    public String getUnnoCd() {
        return unnoCd;
    }
    public void setUnnoCd(String unnoCd) {
        this.unnoCd = unnoCd;
    }
    public String getPtyDivCd() {
        return ptyDivCd;
    }
    public void setPtyDivCd(String ptyDivCd) {
        this.ptyDivCd = ptyDivCd;
    }
    private String ptyDivCd;

    public String getAddr() {
        return addr;
    }
    public void setAddr(String addr) {
        this.addr = addr;
    }
    public String getCompRegNo() {
        return compRegNo;
    }
    public void setCompRegNo(String compRegNo) {
        this.compRegNo = compRegNo;
    }
  
    public String getEngPtyNm() {
        return engPtyNm;
    }
    public void setEngPtyNm(String engPtyNm) {
        this.engPtyNm = engPtyNm;
    }
    public String getFaxNo() {
        return faxNo;
    }
    public void setFaxNo(String faxNo) {
        this.faxNo = faxNo;
    }
    public String getPtyCd() {
        return ptyCd;
    }
    public void setPtyCd(String ptyCd) {
        this.ptyCd = ptyCd;
    }
    public String getRmk() {
        return rmk;
    }
    public void setRmk(String rmk) {
        this.rmk = rmk;
    }
    public String getRpstvNm() {
        return rpstvNm;
    }
    public void setRpstvNm(String rpstvNm) {
        this.rpstvNm = rpstvNm;
    }
    public String getTelNo() {
        return telNo;
    }
    public void setTelNo(String telNo) {
        this.telNo = telNo;
    }
    public String getTmnlTpCd() {
        return tmnlTpCd;
    }
    public void setTmnlTpCd(String tmnlTpCd) {
        this.tmnlTpCd = tmnlTpCd;
    }
    public String getWebSite() {
        return webSite;
    }
    public void setWebSite(String webSite) {
        this.webSite = webSite;
    }    
    public String getCrudFlag() {
        return crudFlag;
    }
    public void setCrudFlag(String crudFlag) {
        this.crudFlag = crudFlag;
    }
    public String getEmail() {
        return email;
    }
    public void setEmail(String email) {
        this.email = email;
    }
}
