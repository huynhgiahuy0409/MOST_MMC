					"renderer": function(v,m,r) {
						if (r.data.workingStatus == 'C') {
							this.items[0].iconCls = 'x-fa fa-file-text-o';
						} else if (r.data.workingStatus == 'U') {
							this.items[0].iconCls = 'x-fa fa-pencil-square-o';
						} else if (r.data.workingStatus == 'D') {
